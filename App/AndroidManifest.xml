<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.base.basket.basketbase2">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".calendario.MainActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".calendario.ClubActivity"
            android:parentActivityName=".calendario.MainActivity"/>
        <activity
            android:name=".calendario.EquiposActivity"
            android:parentActivityName=".calendario.ClubActivity"/>
        <activity
            android:name=".calendario.PartidosActivity"
            android:parentActivityName=".calendario.EquiposActivity"/>
        <activity
            android:name=".patros.MainActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".patros.VerPatro"
            android:parentActivityName=".patros.MainActivity"/>
        <activity
            android:name=".ofertas.MainActivity"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".noticias.VerNoticia"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>
